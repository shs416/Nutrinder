{
    "recipes": [
        {
            "name": "Chicken Alfredo",
            "ingredients": ["2 boneless chicken breasts", "2 tbsp. olive oil", "Salt and pepper", "1 jar of Alfredo's Alfredo Sauce", "1 cup parmesan cheese"],
            "imgLink": "https://hips.hearstapps.com/del.h-cdn.co/assets/17/24/1024x512/landscape-1497458683-delish-one-pot-chicken-alfredo-1-1024.jpg",
            "steps": ["1. Heat oil in large skillet, cook chicken breasts until cooked, then season with salt and pepper", "2. Add pasta and stir in Alfredo's Alfredo Sauce. Simmer until pasta is soft", "3. Remove from heat and serve."]
        },
        {
            "name": "Honey Garlic Chicken",
            "ingredients": ["2 boneless chicken breasts", "2 tbsp honey", "2 tbsp of garlic", "2 tsp olive oil", "2 tsp salt"],
            "imgLink": "https://i1.wp.com/cafedelites.com/wp-content/uploads/2016/09/Honey-Garlic-Chicken-IMAGE-1.jpg?resize=980%2C1470&ssl=1",
            "steps": ["1. Mix honey, garlic, salt, and 1 tbsp olive oil. Marinate chicken breasts in mixture for at least two hours.", "2. Heat remaining olive oil in pan and add chicken breasts.", "Cook for 5 minutes per side, until gently browned. Remove from heat and serve."]
        },
        {
            "name": "Orange Chicken",
            "ingredients": ["2 chicken breasts, diced", "1 orange, squeezed and zested", "2 tbsp soy sauce","1 tbsp garlic", "1 onion, chopped", "2 tbsp brown sugar", "1 cup flour", "2 tbsp olive oil", "1/2 tsp salt and pepper", "1 cup water"],
            "imgLink": "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTiTsGGNP_yG4PmTFxDbUtCz6LGYiu0W7jWXUjcEIC4jofe_tGowQ",
            "steps": ["1. Add orange juice, zest, soy sauce, garlic, onion, brown sugar, and water to sauce pan and bring to boil. Remove from heat and cool for 15 minutes.", "2. Add chicken to bag, mix flour, salt, and pepper", "3. Refrigerate for at least two hours.", "Heat olive oil in pan, and add chicken breasts. Cook until browned on both sides.", "Add cooked chicken breast to sauce and serve."]
        },
        {
            "name": "Chicken Tikka Masala",
            "ingredients": ["2 chicken breasts", "1 jar of Tikka Masala sauce", "1 onion, diced", "1 tbsp olive oil"],
            "imgLink": "https://i0.wp.com/cafedelites.com/wp-content/uploads/2018/04/Best-Chicken-Tikka-Masala-IMAGE-1.jpg?w=1600&ssl=1",
            "steps": ["1. Heat oil in pan, add chicken breasts. Cook 3 minutes per side", "2. Add Tikka Masala sauce and onions, simmer for 15 minutes", "Remove from heat, serve over rice"]
        },
        {
            "name": "Cheesy Spinach Artichoke Bagels",
            "ingredients": ["4 bagels","2 cups spinach, diced", "8 artichoke hearts", "1 cup parmesan cheese", "1 cup pepper jack cheese", "1 cup mayonnaise", "2 tbsp garlic salt"],
            "imgLink": "https://hips.hearstapps.com/hmg-prod.s3.amazonaws.com/images/gallery-1515429625-delish-spinach-artichoke-pizza-bagels-pin-2-1529095463.jpg",
            "steps": ["1. Combine the artichoke hearts, mayonnaise, parmesan cheese, pepper jack cheese, garlic salt, and spinach in a baking tray", "2. Bake, uncovered, at 350 degrees for 20 minutes." "Spread over bagels and serve"]
        },
        {
            "name": "Creamed Spinach Stuffed Salmon",
            "ingredients": ["2 salmon fillets","1 cup spinach, chopped", "8 tbsp cream cheese", "1/4 cup parmesan cheese", "2 tsp salt and pepper", "2 tbsp olive oil"],
            "imgLink": "https://hips.hearstapps.com/hmg-prod.s3.amazonaws.com/images/delish-creamed-spinach-stuffed-salmon-pinterest-still001-1532720411.jpg",
            "steps": ["1. Mix spinach, cream cheese, parmesan cheese, salt, and pepper in bowl", "2. Wrap salmon fillets around mixture","Heat oil in baking tray, add salmon and bake at 350 degrees for 30 minutes." "Remove from heat and serve."]
        },

        {
            "name": "Chicken Noodle Soup",
            "ingredients": ["Campbell's Chicken Noodle Soup", "1 package pasta noodles"],
            "imgLink": "https://hips.hearstapps.com/hmg-prod.s3.amazonaws.com/images/pasta-recipes-chicken-noodle-1541004778.jpg",
            "steps": ["1. Heat Noodle Soup in pot, and add pasta","Bring to boil, until noodles are softened."," Remove from heat and serve."]
        },
        {
            "name": "Anchor Steam Mac and Cheese",
            "ingredients": ["1 package pasta elbows", "4 tbsp Anchor Steam seasoning", "1 cup American Cheese", "1 cup Parmesan Cheese", "4 tbsp butter"],
            "imgLink": "https://hips.hearstapps.com/clv.h-cdn.co/assets/17/19/1494521586-recipes-anchor-steam-mac-and-cheese-0617.jpg",
            "steps": ["1. Add pasta to water and bring to boil for 13 minutes.", "2. Strain pasta and add butter, Anchor Steam seasoning, and cheeses until creamy. Remove from heat and serve."]
